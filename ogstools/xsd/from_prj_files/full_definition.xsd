<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="OpenGeoSysProject">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="meshes"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="geometry"/>
          <xs:element ref="mesh"/>
        </xs:choice>
        <xs:choice minOccurs="0">
          <xs:element ref="python_script"/>
          <xs:element ref="rasters"/>
          <xs:element ref="search_length_algorithm"/>
        </xs:choice>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="include"/>
          <xs:element ref="parameters"/>
          <xs:element ref="process_variables"/>
          <xs:element ref="processes"/>
          <xs:element ref="chemical_system"/>
          <xs:element ref="curves"/>
          <xs:element ref="linear_solvers"/>
          <xs:element ref="local_coordinate_system"/>
          <xs:element ref="media"/>
          <xs:element ref="nonlinear_solvers"/>
          <xs:element ref="time_loop"/>
        </xs:choice>
        <xs:choice minOccurs="0">
          <xs:element ref="insitu"/>
          <xs:element ref="test_definition"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="python_script" type="xs:NCName"/>
  <xs:element name="rasters">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="raster"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="raster">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="file"/>
        <xs:element ref="variable"/>
        <xs:element ref="dimension"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="dimension" type="xs:integer"/>
  <xs:element name="search_length_algorithm">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="type"/>
        <xs:element ref="value"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="chemical_system">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="mesh"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="linear_solver"/>
          <xs:element ref="database"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="use_high_precision"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="number_of_components"/>
          <xs:element ref="chemical_reactions"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="solution"/>
        <xs:choice minOccurs="0">
          <xs:element ref="equilibrium_reactants"/>
          <xs:element ref="exchangers"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="surface"/>
        <xs:choice minOccurs="0">
          <xs:element ref="user_punch"/>
          <xs:sequence>
            <xs:element ref="kinetic_reactants"/>
            <xs:element ref="rates"/>
          </xs:sequence>
        </xs:choice>
        <xs:element minOccurs="0" ref="knobs"/>
      </xs:sequence>
      <xs:attribute name="chemical_solver" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="database" type="xs:string"/>
  <xs:element name="use_high_precision" type="xs:boolean"/>
  <xs:element name="number_of_components" type="xs:integer"/>
  <xs:element name="chemical_reactions">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="chemical_reaction"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="chemical_reaction">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="stoichiometric_coefficients"/>
        <xs:element ref="reaction_type"/>
        <xs:element ref="first_order_rate_constant"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="stoichiometric_coefficients" type="xs:string"/>
  <xs:element name="reaction_type" type="xs:NCName"/>
  <xs:element name="first_order_rate_constant" type="xs:double"/>
  <xs:element name="solution">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="temperature"/>
        <xs:element ref="pressure"/>
        <xs:element ref="pe"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="components"/>
          <xs:element ref="charge_balance"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="fixing_pe" type="xs:boolean"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="pe" type="xs:decimal"/>
  <xs:element name="charge_balance" type="xs:NCName"/>
  <xs:element name="equilibrium_reactants">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="phase_component"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="phase_component">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name"/>
        <xs:element minOccurs="0" ref="initial_amount"/>
        <xs:element ref="saturation_index"/>
        <xs:element minOccurs="0" ref="reaction_irreversibility"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="saturation_index" type="xs:decimal"/>
  <xs:element name="reaction_irreversibility" type="xs:NCName"/>
  <xs:element name="exchangers">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="exchange_site"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="exchange_site">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ion_exchanging_species"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="ion_exchanging_species" type="xs:NCName"/>
  <xs:element name="surface">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="site"/>
      </xs:sequence>
      <xs:attribute name="site_unit" use="required" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="site">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="site_density"/>
          <xs:element ref="specific_surface_area"/>
          <xs:element ref="mass"/>
        </xs:sequence>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="site_density" type="xs:decimal"/>
  <xs:element name="specific_surface_area" type="xs:decimal"/>
  <xs:element name="mass" type="xs:integer"/>
  <xs:element name="user_punch">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="headline"/>
        <xs:element maxOccurs="unbounded" ref="statement"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="headline" type="xs:string"/>
  <xs:element name="kinetic_reactants">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="kinetic_reactant"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="rates">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="rate"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="rate">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="kinetic_reactant"/>
        <xs:element ref="expression"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="knobs">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="max_iter"/>
        <xs:element ref="relative_convergence_tolerance"/>
        <xs:element ref="tolerance"/>
        <xs:element ref="step_size"/>
        <xs:element ref="scaling"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="relative_convergence_tolerance" type="xs:double"/>
  <xs:element name="tolerance" type="xs:double"/>
  <xs:element name="step_size" type="xs:integer"/>
  <xs:element name="curves">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="curve"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="linear_solvers">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="linear_solver"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="local_coordinate_system">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="basis_vector_0"/>
        <xs:element ref="basis_vector_1"/>
        <xs:element minOccurs="0" ref="basis_vector_2"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="basis_vector_0">
    <xs:complexType mixed="true">
      <xs:attribute name="implicit" type="xs:boolean"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="basis_vector_1" type="xs:NCName"/>
  <xs:element name="basis_vector_2" type="xs:NCName"/>
  <xs:element name="media">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="medium"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="medium">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="phases"/>
        <xs:element minOccurs="0" ref="properties"/>
      </xs:sequence>
      <xs:attribute name="id"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="phases">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="phase"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="phase">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="type"/>
        <xs:element minOccurs="0" ref="components"/>
        <xs:element minOccurs="0" ref="properties"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="nonlinear_solvers">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="nonlinear_solver"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="time_loop">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="global_process_coupling"/>
        <xs:element minOccurs="0" ref="processes"/>
        <xs:element minOccurs="0" ref="include"/>
        <xs:element minOccurs="0" ref="output"/>
        <xs:choice minOccurs="0">
          <xs:element ref="outputs"/>
          <xs:element ref="submesh_residuum_output"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="global_process_coupling">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="max_iter"/>
        <xs:element ref="convergence_criteria"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="convergence_criteria">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="convergence_criterion"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="outputs">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="output"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="submesh_residuum_output">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="type"/>
        <xs:element minOccurs="0" ref="prefix"/>
        <xs:element ref="timesteps"/>
        <xs:element ref="variables"/>
        <xs:element minOccurs="0" ref="suffix"/>
        <xs:element ref="meshes"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="insitu">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="scripts"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="scripts">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="script"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="script">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="test_definition">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="vtkdiff"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="vtkdiff">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="file"/>
          <xs:element ref="regex"/>
        </xs:choice>
        <xs:element ref="field"/>
        <xs:element ref="absolute_tolerance"/>
        <xs:element ref="relative_tolerance"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="regex" type="xs:string"/>
  <xs:element name="field" type="xs:string"/>
  <xs:element name="absolute_tolerance" type="xs:double"/>
  <xs:element name="relative_tolerance" type="xs:double"/>
  <xs:element name="meshes">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="mesh"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="geometry" type="xs:string"/>
  <xs:element name="mesh">
    <xs:complexType mixed="true">
      <xs:attribute name="axially_symmetric" type="xs:boolean"/>
      <xs:attribute name="material_ids"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="type" type="xs:NMTOKEN"/>
  <xs:element name="value">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="expression"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="file" type="xs:NCName"/>
  <xs:element name="variable" type="xs:string"/>
  <xs:element name="include">
    <xs:complexType>
      <xs:attribute name="file" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="max_iter" type="xs:integer"/>
  <xs:element name="convergence_criterion">
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="type"/>
          <xs:element ref="norm_type"/>
        </xs:choice>
        <xs:choice>
          <xs:choice maxOccurs="unbounded">
            <xs:element ref="abstol"/>
            <xs:element ref="reltol"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded">
            <xs:element ref="abstols"/>
            <xs:element ref="reltols"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="norm_type" type="xs:NCName"/>
  <xs:element name="abstol" type="xs:double"/>
  <xs:element name="reltol" type="xs:double"/>
  <xs:element name="abstols" type="xs:string"/>
  <xs:element name="reltols" type="xs:string"/>
  <xs:element name="processes">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="include"/>
        <xs:element maxOccurs="unbounded" ref="process"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="process">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="name"/>
          <xs:element ref="type"/>
        </xs:sequence>
        <xs:element minOccurs="0" ref="subtype"/>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="process_variables"/>
          <xs:element ref="reference_temperature"/>
          <xs:element ref="apply_body_force_for_deformation"/>
          <xs:element ref="calculatesurfaceflux"/>
          <xs:element ref="characteristic_length"/>
          <xs:element ref="chemically_induced_porosity_change"/>
          <xs:element ref="constitutive_relation"/>
          <xs:element ref="coupling_scheme"/>
          <xs:element ref="deactivate_matrix_in_flow"/>
          <xs:element ref="energy_split_model"/>
          <xs:element ref="equation_balance_type"/>
          <xs:element ref="explicit_hm_coupling_in_unsaturated_zone"/>
          <xs:element ref="f_bar"/>
          <xs:element ref="fracture_model"/>
          <xs:element ref="fracture_properties"/>
          <xs:element ref="ice_constitutive_relation"/>
          <xs:element ref="initial_effective_stress"/>
          <xs:element ref="initial_fracture_effective_stress"/>
          <xs:element ref="initial_stress"/>
          <xs:element ref="integration_order"/>
          <xs:element ref="internal_length"/>
          <xs:element ref="irreversible_threshold"/>
          <xs:element ref="jacobian_assembler"/>
          <xs:element ref="mass_lumping"/>
          <xs:element ref="micro_porosity"/>
          <xs:element ref="non_advective_form"/>
          <xs:element ref="phasefield_model"/>
          <xs:element ref="phasefield_parameters"/>
          <xs:element ref="pressurized_crack_scheme"/>
          <xs:element ref="secondary_variables"/>
          <xs:element ref="softening_curve"/>
          <xs:element ref="solid_density"/>
          <xs:element ref="specific_body_force"/>
          <xs:element ref="tabular_file"/>
          <xs:element ref="thermal_parameters"/>
          <xs:element ref="use_b_bar"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="numerical_stabilization"/>
        <xs:element minOccurs="0" ref="use_server_communication"/>
        <xs:choice minOccurs="0">
          <xs:element ref="temperature"/>
          <xs:element ref="borehole_heat_exchangers"/>
          <xs:element ref="simplified_elasticity"/>
          <xs:element ref="temperature_field"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="aperture_size"/>
        <xs:choice>
          <xs:element ref="linear"/>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="convergence_criterion"/>
            <xs:element ref="nonlinear_solver"/>
            <xs:element ref="time_discretization"/>
            <xs:element ref="time_stepping"/>
          </xs:choice>
          <xs:sequence>
            <xs:element ref="wellbore"/>
            <xs:element ref="wellbore_ref_pressure"/>
            <xs:element ref="wellbore_ref_enthalpy"/>
            <xs:element ref="heat_exchange_with_formation"/>
            <xs:element ref="reservoir_properties"/>
            <xs:element ref="productivity_index"/>
          </xs:sequence>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="ref" type="xs:NCName"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="subtype" type="xs:NCName"/>
  <xs:element name="apply_body_force_for_deformation" type="xs:boolean"/>
  <xs:element name="calculatesurfaceflux">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="mesh"/>
        <xs:element ref="property_name"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="characteristic_length" type="xs:decimal"/>
  <xs:element name="chemically_induced_porosity_change" type="xs:boolean"/>
  <xs:element name="constitutive_relation">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="type"/>
        <xs:sequence minOccurs="0">
          <xs:element ref="youngs_modulus"/>
          <xs:element ref="poissons_ratio"/>
        </xs:sequence>
        <xs:sequence minOccurs="0">
          <xs:element ref="a"/>
          <xs:element ref="n"/>
          <xs:element ref="sigma0"/>
          <xs:element ref="q"/>
        </xs:sequence>
        <xs:choice minOccurs="0">
          <xs:sequence>
            <xs:element ref="youngs_modulus_i"/>
            <xs:element ref="youngs_modulus_a"/>
            <xs:element ref="poissons_ratio_ii"/>
            <xs:element ref="poissons_ratio_ia"/>
            <xs:element ref="shear_modulus_ia"/>
          </xs:sequence>
          <xs:sequence>
            <xs:element ref="youngs_moduli"/>
            <xs:choice maxOccurs="unbounded">
              <xs:element ref="poissons_ratios"/>
              <xs:element ref="shear_moduli"/>
            </xs:choice>
          </xs:sequence>
        </xs:choice>
        <xs:sequence minOccurs="0">
          <xs:element ref="shear_modulus"/>
          <xs:element ref="bulk_modulus"/>
          <xs:element ref="kappa"/>
          <xs:element ref="beta"/>
          <xs:element ref="gamma"/>
          <xs:element ref="hardening_modulus"/>
          <xs:element ref="alpha"/>
          <xs:element ref="delta"/>
          <xs:element ref="eps"/>
          <xs:element ref="m"/>
          <xs:element ref="alphap"/>
          <xs:element ref="deltap"/>
          <xs:element ref="epsp"/>
          <xs:element ref="mp"/>
          <xs:element ref="betap"/>
          <xs:element ref="gammap"/>
          <xs:element ref="tangent_type"/>
        </xs:sequence>
        <xs:choice minOccurs="0">
          <xs:element ref="damage_properties"/>
          <xs:sequence>
            <xs:element ref="kelvin_shear_modulus"/>
            <xs:element ref="kelvin_viscosity"/>
            <xs:element ref="maxwell_shear_modulus"/>
            <xs:element ref="maxwell_bulk_modulus"/>
            <xs:element ref="maxwell_viscosity"/>
            <xs:element ref="dependency_parameter_mk"/>
            <xs:element ref="dependency_parameter_mvk"/>
            <xs:element ref="dependency_parameter_mvm"/>
          </xs:sequence>
        </xs:choice>
        <xs:element minOccurs="0" ref="nonlinear_solver"/>
        <xs:element minOccurs="0" ref="behaviour"/>
        <xs:choice minOccurs="0">
          <xs:element ref="library"/>
          <xs:element ref="material_properties"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="initial_values"/>
      </xs:sequence>
      <xs:attribute name="id"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="a" type="xs:NCName"/>
  <xs:element name="n" type="xs:NCName"/>
  <xs:element name="sigma0" type="xs:NCName"/>
  <xs:element name="q" type="xs:NCName"/>
  <xs:element name="youngs_modulus_i" type="xs:NCName"/>
  <xs:element name="youngs_modulus_a" type="xs:NCName"/>
  <xs:element name="poissons_ratio_ii" type="xs:NCName"/>
  <xs:element name="poissons_ratio_ia" type="xs:NCName"/>
  <xs:element name="shear_modulus_ia" type="xs:NCName"/>
  <xs:element name="youngs_moduli" type="xs:NCName"/>
  <xs:element name="poissons_ratios" type="xs:NCName"/>
  <xs:element name="shear_moduli" type="xs:NCName"/>
  <xs:element name="shear_modulus" type="xs:NCName"/>
  <xs:element name="bulk_modulus" type="xs:NCName"/>
  <xs:element name="kappa" type="xs:NCName"/>
  <xs:element name="beta" type="xs:NCName"/>
  <xs:element name="gamma" type="xs:NCName"/>
  <xs:element name="hardening_modulus" type="xs:NCName"/>
  <xs:element name="delta" type="xs:NCName"/>
  <xs:element name="eps" type="xs:NCName"/>
  <xs:element name="m" type="xs:NCName"/>
  <xs:element name="alphap" type="xs:NCName"/>
  <xs:element name="deltap" type="xs:NCName"/>
  <xs:element name="epsp" type="xs:NCName"/>
  <xs:element name="mp" type="xs:NCName"/>
  <xs:element name="betap" type="xs:NCName"/>
  <xs:element name="gammap" type="xs:NCName"/>
  <xs:element name="tangent_type" type="xs:NCName"/>
  <xs:element name="damage_properties">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="alpha_d"/>
        <xs:element ref="beta_d"/>
